            <datadir zip="true">https://raw.githubusercontent.com/Laser78/PLUGINSXBMC/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repositorio Oficial de PLUGINSXBMC</summary>
            <description></description>
            <platform>all</platform>
    </extension>
</addon>

