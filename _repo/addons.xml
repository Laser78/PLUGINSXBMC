<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.audio.di.fm"
       name="Digitally Imported"
       version="4.1.2"
       provider-name="qualisoft.dk - Tim C. Steinmetz">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<language>en</language>
		<license>GPL 2.0</license>
		<forum>http://forum.xbmc.org/showthread.php?tid=129857</forum>
    		<website>http://www.qualisoft.dk</website>
		<source>https://github.com/Bitcrusher/Digitally-Imported-XBMC-plugin</source>
   		<summary lang="en">Digitally Imported</summary>
		<description lang="en">
			Digitally Imported streams the world's most addictive electronic music. Our radio channels feature world renowned artists, DJs, and the hottest exclusive shows. Discover the community and the wide variety of electronic music covering Trance, House, Dance, Techno, Chillout, Breaks, Downtempo and so much more.
		</description>
		<description lang="de">
			Digitally Imported streamt süchtig machende elektronische Musik. Unsere Sender präsentieren neben Werken der renommiertesten Künstler und DJs der Welt exklusive Shows. Entdecken Sie die Community und die breite Vielfalt elektronischer Musik, welche unter anderem Trance, House, Dance, Techno, Chillout, Breaks, Downtempo und viele mehr umfasst.
		</description>
	</extension>
</addon>

<addon id="plugin.video.xbmctools" name="KODI tools" version="1.3.0" provider-name="dcatx">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>executable</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" start="startup">
    </extension>
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">Actualizador de Librerias</summary>
    <description lang="en">Herramientas para actualizar las Libtmp.dll </description>
    <platform>all</platform>
  </extension>
</addon>

    <addon id="PLUGINSXBMC" name="PLUGINSXBMC REPO" version="1.9.0" provider-name="LASER">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="PLUGINSXBMC REPO">
            <info compressed="false">https://raw.githubusercontent.com/Laser78/PLUGINSXBMC/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Laser78/PLUGINSXBMC/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Laser78/PLUGINSXBMC/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repositorio Oficial de PLUGINSXBMC</summary>
            <description></description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
